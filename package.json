  "description": "This is can use prompt easy and colorful",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "npm-run-all clean cjs esm",
